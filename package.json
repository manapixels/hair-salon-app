{
  "name": "hair-salon-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint:fix && npm run format && npm run typecheck",
    "inngest": "npx inngest-cli@latest dev",
    "prepare": "husky install",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "admin:create": "node -e \"require('./scripts/create-admin.js')\"",
    "data:backup": "node -e \"console.log('Manual backup: Use Neon dashboard or pg_dump')\"",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
